<h1> Database </h1>
	<%= link_to "Create", new_database_path %>
<table>
	<thead>
		<th> Title </th>
		<th> Description </th>
	</thead>
	<% @csv.each do |csv| %>
	<tr>
		<td> <%=csv.title %></td>
		<td> <%=csv.description %> </td>

		<td>
			<%= link_to "Show", database_path(csv) %>
			<%= link_to "Edit", edit_database_path(csv) %>
			<%= link_to "Destroy", database_path(csv), method: :delete, data: {confirm:"Are you sure?"}  %>
		</td>
	</tr>
	<% end %>
</table>